{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "Profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "bio": {
      "type": "text"
    },
    "linkedin": {
      "type": "string"
    },
    "github": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profile"
    },
    "profilepicture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "name": {
      "type": "string"
    }
  }
}
