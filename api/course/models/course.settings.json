{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "course"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "github_repo": {
      "type": "string"
    },
    "language": {
      "model": "language"
    },
    "lectures": {
      "collection": "lecture",
      "via": "course"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "courses"
    }
  }
}
